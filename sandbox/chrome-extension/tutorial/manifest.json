{
  "manifest_version": 2,

  "name": "Getting start example",
  "description": "Shows a Google Image search result of current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
